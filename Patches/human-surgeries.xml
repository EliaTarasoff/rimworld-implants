<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef["defName=Human" and @ParentName="BasePawn"]/recipes</xpath>
        <value>
            <li>InstallStomachSymbiote</li>
            <li>RemoveStomachSymbiote</li>

            <li>InstallHeartSymbiote</li>
            <li>RemoveHeartSymbiote</li>

            <li>InstallEarSymbiote</li>
            <li>RemoveEarSymbiote</li>

            <li>InstallSpineSymbiote</li>
            <li>RemoveSpineSymbiote</li>

            <!--things which subclass bionic parts can be removed by default, so only need install recipes?-->
            <li>InstallToxicLung</li>
            <li>InstallStimulantKidney</li>

            <li>InstallWoodenNose</li>
            <li>InstallWoodenHand</li>
        </value>
    </Operation>
</Patch>
